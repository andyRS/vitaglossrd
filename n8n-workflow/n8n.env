# ════════════════════════════════════════════════════
# Configuración de n8n para VitaGloss RD
# Cómo usar: en el script setup-n8n.ps1 se carga automáticamente
# ════════════════════════════════════════════════════

# Puerto y host
N8N_PORT=5678
N8N_HOST=localhost
N8N_PROTOCOL=http

# Seguridad — usuario/contraseña para acceder al panel n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=vitagloss2026

# Habilitar API REST de n8n (para crear variables programáticamente)
N8N_PUBLIC_API_DISABLED=false

# Timezone
GENERIC_TIMEZONE=America/Santo_Domingo

# Webhook base URL (URL donde n8n escucha webhooks)
WEBHOOK_URL=http://localhost:5678/

# No abrir browser automáticamente
N8N_OPEN_BROWSER=false

# Guardar ejecuciones completadas (útil para debugging)
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true

